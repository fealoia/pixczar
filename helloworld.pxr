Void main() {
    //Initializing 5 seconds of frames
    Frame[] frames = new Frame[150];

    Pix ball = new Pix();
    ball.makeEllipse(300, 300, [255, 255, 255]);

    //Place ball in middle of screen
    Placement placeBall = new Placement(ball, 375, 375, 1, 1);
    
    Int i = 0;
    for(i; i < 150; i = i + 1) {
        frames[0] = new Frame(750, 750);
        frames[0].addPlacement(placeBall);
    }

    Int fps = 30;
    render(frames, fps);
}
